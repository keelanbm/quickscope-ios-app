{
  "expo": {
    "name": "Quickscope",
    "slug": "quickscope-ios-app",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "quickscope",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "runtimeVersion": "0.1.0",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.quickscope.iosapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.quickscope.iosapp"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-web-browser",
      "expo-dev-client"
    ],
    "extra": {
      "projectName": "quickscope-ios-app",
      "eas": {
        "projectId": "432c9fe0-204a-4b76-9e33-fde84aba4946"
      }
    },
    "owner": "keelanbm"
  }
}
